{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.SpaProxy": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=127.0.0.1;Database=AuthDatabase;Username=admin;Password=admin1234;Port=5100;Pooling=true;"
  },
  "AllowedHosts": "*",
  "JwtOptions": {
    "SecurityKey": "9{&%s/SDPu}hcov=(/ouie8]2iouei!bR,i^V)H>RieuibzB.VXzvDuieoeou|m4[4p2L8.{0rU)d",
    "Issuer": "http://127.0.0.1",
    "Audience": "http://127.0.0.1",
    "ExpiryInDays": 1
  },
  "IdentityOptions": {
    "Password": {
      "RequireDigit": true,
      "RequireLowercase": true,
      "RequireNonAlphanumeric": true,
      "RequireUppercase": true,
      "RequiredLenght": 8,
      "RequiredUniqueChars": 1
    },
    "Lockout": {
      "DefaultLockoutTimeSpanInMs": 300000,
      "MaxFailedAccessAttempts": 5,
      "AllowedForNewUsers": true,
      "LockoutOnFailure": true
    },
    "User": {
      "RequireUniqueEmail": true,
      "AllowedUserNameCharacters": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789-._@+"
    },
    "SignIn": {
      "RequireConfirmedPhoneNumber": false,
      "RequireConfirmedAccount": false,
      "RequireConfirmedEmail": false
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  },
  "CultureInfo": {
    "DefaultCulture": "en",
    "SupportedCultures": [
      "en",
      "fr"
    ]
  }
}
